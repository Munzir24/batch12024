pipeline {
    agent {
        label 'slave1'
    }
    stages {
        stage('Working with conditions') {
            steps {
                script {
                    i=1
                    while (i <=17.5) {
                        println "Value of i in this run is ${i}"
                        i=i+1.25
                    }
                    }
                    
                    
                }
            }
        }
    }
}
