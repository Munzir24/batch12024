pipeline {
    agent {
        label 'slave1'
    }
    stages {
        stage('Working with conditions') {
            steps {
                script {
                    list1 = [10,20,30,40]
                    for (element in list1) {
                        println "The elements are ${element}"
                    }
                    
                    
                }
            }
        }
    }
}
